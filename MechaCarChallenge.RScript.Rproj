## Deliverable 1
library(dplyr)
library(tidyverse)
mecha_mpg <- read.csv(file='./MechaCar_mpg.csv', check.name = F, stringAsFactors = F)
lm(mpg ~ vehicle_length + vehicle_weight + spoiler_angle + ground_clearance + AWD, data = mecha_mpg)
summary(lm(mpg ~ vehicle_length + vehicle_weight + spoiler_angle + ground_clearance + AWD, data = mecha_mpg))

## Deliverable 2
mecha_coil <- read.csv(file='./Suspension_Coil.csv', check.name = F, stringAsFactors = F)
total_summary <- mecha_coil %>% summarize(Mean=mean(PSI), Median=median(PSI), Variance=var(PSI), Std_Dev=sd(PSI))
lot_summary  <- mecha_coil %>% group_by(Manufacturing_Lot) %>% summarize(Mean=mean(PSI), Median=median(PSI), Variance=var(PSI), Std_Dev=sd(PSI))
